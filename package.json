{
  "name": "ordinem",
  "main": "expo-router/entry",
  "version": "2.0.0",
  "homepage": "https://oriolgds.github.io/ordinem",
  "scripts": {
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "prebuild": "expo prebuild",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "prebuild-clean": "expo prebuild --clean",
    "build:android": "eas build --platform android --profile production",
    "build:android-local": "eas build --platform android --profile production --local",
    "build:aab": "eas build --platform android --profile production",
    "build:aab-local": "eas build --platform android --profile production --output=build.aab --local",
    "validate:bundle": "./scripts/validate-bundle.sh",
    "workflow:run": "npx eas-cli workflow:run",
    "workflow:android": "npx eas-cli workflow:run .eas/workflows/android-build.yml",
    "predeploy": "expo export",
    "deploy": "gh-pages -d dist",
    "full-deploy": "npm run predeploy && npm run deploy"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "~5.1.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-google-signin/google-signin": "~13.2.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "~7.2.10",
    "expo": "53.0.20",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.4",
    "expo-dev-launcher": "^5.1.12",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-modules-core": "~2.5.0",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "firebase": "^11.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-picker-select": "~9.3.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "~7.25.2",
    "@types/jest": "~29.5.12",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/react-test-renderer": "~18.3.0",
    "babel-plugin-module-resolver": "~5.0.2",
    "eas-cli": "^16.17.4",
    "gh-pages": "~6.3.0",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.3.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "expo-barcode-scanner",
          "firebase"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
