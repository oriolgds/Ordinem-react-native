{
    "cli": {
        "version": ">= 5.4.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "gradleCommand": ":app:assembleDebug"
            }
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "android": {
                "buildType": "apk",
                "autoIncrement": true
            },
            "ios": {
                "distribution": "store",
                "autoIncrement": true
            }
        }
    },
    "submit": {
        "production": {}
    }
}